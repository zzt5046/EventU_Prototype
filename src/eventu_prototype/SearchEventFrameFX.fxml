            <RadioButton fx:id="categoryRadio" layoutX="39.0" layoutY="193.0" mnemonicParsing="false" text="Category Search" />
            <TextField layoutX="163.0" layoutY="113.0" prefHeight="25.0" prefWidth="108.0" promptText="Search Term" />
