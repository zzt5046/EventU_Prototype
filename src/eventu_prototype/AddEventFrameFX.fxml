<VBox prefHeight="511.0" prefWidth="312.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eventu_prototype.GUIController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="500.0" prefWidth="305.0" style="-fx-background-color: #c6c6c6;" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="searchCancelButton" layoutX="84.0" layoutY="425.0" mnemonicParsing="false" onAction="#eventCancel" prefHeight="32.0" prefWidth="142.0" text="CANCEL" />
            <Text fill="#1b76f5" layoutX="75.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Event">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Button fx:id="addEventButton" layoutX="84.0" layoutY="386.0" mnemonicParsing="false" onAction="#addEvent" prefHeight="32.0" prefWidth="142.0" text="ADD EVENT" />
            <TextField fx:id="eventNameField" layoutX="81.0" layoutY="96.0" promptText="Name" />
            <TextField fx:id="startTimeField" layoutX="81.0" layoutY="122.0" promptText="Start Time" />
            <TextField fx:id="endTimeField" layoutX="81.0" layoutY="147.0" promptText="End Time" />
            <TextField fx:id="locationField" layoutX="81.0" layoutY="174.0" promptText="Location" />
            <TextField fx:id="dateField" layoutX="81.0" layoutY="202.0" promptText="Date MM/DD/YYYY" />
            <TextField fx:id="priceField" layoutX="81.0" layoutY="347.0" promptText="Price $X.XX" />
            <TextArea fx:id="descField" layoutX="81.0" layoutY="262.0" prefHeight="78.0" prefWidth="149.0" promptText="Description" />
            <ComboBox fx:id="addEventCategory" layoutX="81.0" layoutY="231.0" prefWidth="150.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
